#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int A[10][10];
    int i,j,flag=0,count=0;
    int m,n;
    scanf("%d",&m);
    scanf("%d",&n);
    for(i=0;i<m;i++)
      {
          for(j=0;j<n;j++)
              {
              scanf("%d ",&A[i][j]);
              }
             
      }
    for(i=0;i<m;i++)
        {
        for(j=0;j<n;j++)
            {
            if(A[i][j] % 2 == 0)
            flag=flag+1;
            else
                count=count+1;
            }
        }
    printf("%d\n",count);
    printf("%d\n",flag);

              
    
    
            
    

       
    return 0;
}
