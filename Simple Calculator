#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a,b,c;
    scanf("%d\n",&a);
    scanf("%d\n",&b);
    c=a+b;
printf("%d\n",c);
    c=a-b;
    printf("%d\n",c);
    c=a*b;
    printf("%d\n",c);
float d;
    d=(float)a/b;
printf("%.2f",d);
    return 0;
}
